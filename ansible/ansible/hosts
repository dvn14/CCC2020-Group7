[HOSTS]

[HOSTS:vars]
ansible_python_interpreter=/usr/bin/python3
ansible_user=ubuntu
ansible_ssh_private_key_file=/etc/ansible/id_rsa.pem
ansible_ssh_common_args='-o StrictHostKeyChecking=no'

[WEBHOSTS]

[WEBHOSTS:vars]
ansible_python_interpreter=/usr/bin/python3
ansible_user=ubuntu
ansible_ssh_private_key_file=/etc/ansible/id_rsa.pem
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
